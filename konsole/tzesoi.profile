[Appearance]
AntiAliasFonts=true
BoldIntense=true
ColorScheme=Pastel1
Font=Bitstream Vera Sans Mono,9,-1,5,50,0,0,0,0,0

[Encoding Options]
DefaultEncoding=UTF-8

[General]
Directory=~
Environment=TERM=xterm
LocalTabTitleFormat=%# - %d : %n
MenuIndex=2
Name=tzesoi
Parent=FALLBACK/
RemoteTabTitleFormat=%# - %h : %u
ShowMenuBar=true
ShowNewAndCloseTabButtons=true
TabBarPosition=1

[Keyboard]
KeyBindings=default

[Scrolling]
HistoryMode=1
HistorySize=6000
ScrollBarPosition=2

[Terminal Features]
BidiRenderingEnabled=true
BlinkingTextEnabled=false
