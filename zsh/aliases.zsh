# ls with color
alias lc='ls --color=auto'

# ATI utils
alias atitemp='aticonfig --odgt'
alias atiload='aticonfig --od-getclocks'

# Opening
alias go="gnome-open"
alias xo="xdg-open"

# Vim
alias vim='vim -X'

# GSoC
alias soc='cd ~/kde/digikam'
alias socb='cd ~/kde/digikam/build'
alias kipi='cd ~/kde/digikam/extra/libkipi'
alias kipib='cd ~/kde/digikam/extra/libkipi/build'

# cd
alias ...='cd ../..'
alias ....='cd ../../..'
